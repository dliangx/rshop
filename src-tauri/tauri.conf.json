{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "rshop",
  "version": "0.1.0",
  "identifier": "com.liang.rshop",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://192.168.2.4:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "rshop",
        "width": 390,
        "height": 844,
        "resizable": false,
        "center": true,
        "decorations": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "iOS": {
      "developmentTeam": "2T8BMHVXQY"
    }
  }
}
